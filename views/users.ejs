<% include partials/header %>
  <div class="row">
    <div class="col-10 col">
      <h1>
        <%= title %>
      </h1>
    </div>
  </div>

  <br>
  <table>
    <thead>
      <tr>
        <th>_id</th>
        <th>Username</th>
      </tr>
    </thead>
    <tbody>

      <% for (let x = 0; x < users.length; x++) { %>
        <tr>
          <td>
            <%= users[x]._id %>
          </td>
          <td>
            <%= users[x].username %>
          </td>
        </tr>
        <% } %>
    </tbody>
  </table>

  
  <% include partials/footer %>
